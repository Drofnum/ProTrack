@model  ProTrack.Models.Betas.ApplicantDetailListingModel
@{
    ViewData["Title"] = "ApplicantDetails";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>ApplicantDetails</h1>
<h3>@Html.DisplayFor(model => model.FirstName)</h3>
<div>
    <dl class="row">
        <dt class="col-sm-2">
            Email Address:
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Email)
        </dd>
        <dt class="col-sm-2">
            Company:
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.CompanyName)
        </dd>
        <dt class="col-sm-2">
            Shipping Address:
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.StreetAddress)<br />
            @Html.DisplayFor(model => model.City),
            @Html.DisplayFor(model => model.State) @Html.DisplayFor(model => model.PostalCode)<br />
            @Html.DisplayFor(model => model.Country)<br />

        </dd>
        <dt class="col-sm-2">
            Feedback Rating: 
        </dt>
        <dd class="col-sm-10">
            ****
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Applicants" asp-route-id="@Model.Id">Back to List</a>
</div>